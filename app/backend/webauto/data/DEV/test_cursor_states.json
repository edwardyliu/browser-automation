{
    "name": "TEST CURSOR STATES",
    "env": "DEV",
    "commands": [
        ["GET", {"target": "https://www.google.com/"}],

        ["MOVE_BY_OFFSET", {"argv": [100, 200]}],
        ["CLICK"], ["PAUSE", 1.5],
        ["MOVE_BY_OFFSET", {"argv": [400, 400]}],
        ["DOUBLE_CLICK"], ["PAUSE", 1.5],
        ["CLICK_AND_HOLD"], ["PAUSE", 1.5],
        ["RELEASE"],
        
        ["SEND_KEYS", {"target": "//input[@name='q']", "argv": ["Python Selenium ActionChains Documentation${ENTER}"]}],
        ["PAUSE", 3.0],
        ["GET", "https://www.google.com/"],
        ["SEND_KEYS", {"target": "//input[@name='q']", "argv": [["KEY_DOWN", "${SHIFT}"], "lowercase", ["KEY_UP", "${SHIFT}"], "${ENTER}"]}],
        ["PAUSE", 3.0]
    ]
}